    if existing then existing:Destroy() end
    
    local bg = Instance.new("BillboardGui", head)
    bg.Name = "JHub_Tag"
    bg.Adornee = head
    bg.Size = UDim2.new(0, 200, 0, 50)
    bg.ExtentsOffset = Vector3.new(0, 3, 0)
    bg.AlwaysOnTop = true
    
    local tl = Instance.new("TextLabel", bg)
    tl.Size = UDim2.new(1, 0, 1, 0)
    tl.BackgroundTransparency = 1
    tl.Text = txt
    tl.Font = Enum.Font.SourceSansBold
    tl.TextSize = 25
    tl.TextStrokeTransparency = 0
    
    local cols = {Color3.new(1,0,0), Color3.new(0,0,1), Color3.new(0.6,0,1)}
    _0xR.Heartbeat:Connect(function()
        if not tl or not tl.Parent then return end
        local c = cols[math.floor((tick()*4)%#cols)+1]
        tl.TextColor3 = c
    end)
end

