local function _applyTag(_char)
    local _head = _char:WaitForChild("Head", 5)
    if not _head then return end
    if _head:FindFirstChild("JotaTag") then _head.JotaTag:Destroy() end
    
    local _bg = Instance.new("BillboardGui", _head)
    _bg.Name = "JotaTag"
    _bg.Size = UDim2.new(0, 150, 0, 40)
    _bg.AlwaysOnTop = true
    _bg.ExtentsOffset = Vector3.new(0, 3, 0)
    
    local _txt = Instance.new("TextLabel", _bg)
    _txt.Size = UDim2.new(1, 0, 1, 0)
    _txt.BackgroundTransparency = 1
