    local r = ch:FindFirstChild("HumanoidRootPart"); local h = ch:FindFirstChildOfClass("Humanoid")
    
    -- FUNÇÕES DE AUTO-USO (SELF)
    if cmd == "anti-afk" then
        _lp.Idled:Connect(function() _g:GetService("VirtualUser"):CaptureController(); _g:GetService("VirtualUser"):ClickButton2(Vector2.new()) end)
    elseif cmd == "god mode" then
        h.MaxHealth = 9e9; h.Health = 9e9
    elseif cmd == "invisible" then
        for _,v in pairs(_lp.Character:GetDescendants()) do if v:IsA("BasePart") or v:IsA("Decal") then v.Transparency = 1 end end
    elseif cmd == "btools" then
        local t1 = Instance.new("HopperBin", _lp.Backpack); t1.BinType = 4
    elseif cmd == "full bright" then
        _g:GetService("Lighting").Brightness = 2; _g:GetService("Lighting").ClockTime = 14; _g:GetService("Lighting").GlobalShadows = false
    elseif cmd == "speed boost" then
        _lp.Character.Humanoid.WalkSpeed = 100
    elseif cmd == "jump boost" then
        _lp.Character.Humanoid.JumpPower = 150
    elseif cmd == "lag server" then
        for i=1, 500 do Instance.new("Message", workspace).Text = "JHub LAG SYSTEM" task.wait() end
    end

    -- FUNÇÕES DE ALVO
    if not r or not h then return end
    if cmd == "kill" then h.Health = 0
    elseif cmd == "explode" then local e = Instance.new("Explosion", workspace); e.Position = r.Position
    elseif cmd == "void" then r.CFrame = CFrame.new(r.Position.X, -1000, r.Position.Z)
    elseif cmd == "freeze" then r.Anchored = true
    elseif cmd == "unfreeze" then r.Anchored = false
    elseif cmd == "kick" then _lp:Kick("JHub V60")
    elseif cmd == "bring" then r.CFrame = _lp.Character.HumanoidRootPart.CFrame
    elseif cmd == "fling" then h.PlatformStand = true; r.Velocity = Vector3.new(0, 5000, 0)
    elseif cmd == "blind" then 
        local s = Instance.new("ScreenGui", tp.PlayerGui); local f = Instance.new("Frame", s); f.Size = UDim2.new(2,0,2,0); f.BackgroundColor3 = Color3.new(0,0,0); task.wait(5); s:Destroy()
    elseif cmd == "verify" then _g:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer("JHub V60 ✅", "All")
    end
end

